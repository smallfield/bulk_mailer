<queue-entries>
	<queue>
		<name>default</name>
		<rate>10/s</rate>
	</queue>
	<queue>
		<name>mail</name>
		<rate>5/s</rate>
		<retry-parameters>
			<task-retry-limit>1</task-retry-limit>
			<task-age-limit>1s</task-age-limit>
		</retry-parameters>
	</queue>
	<queue>
		<name>slim3-gtx-queue</name>
		<rate>10/s</rate>
		<bucket-size>10</bucket-size>
	</queue>
</queue-entries>